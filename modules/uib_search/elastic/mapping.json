{
  "settings": {
    "analysis": {
      "filter": {
        "trigrams_filter": {
          "type": "ngram",
          "min_gram": 3,
          "max_gram": 3
        },
        "shingle_filter": {
          "type": "shingle",
          "min_shingle_size": 2,
          "max_shingle_size": 2,
          "output_unigrams":  false
        }
      },
      "analyzer": {
        "trigrams": {
          "type": "custom",
          "tokenizer": "standard",
          "filter":   [
            "lowercase",
            "trigrams_filter",
            "shingle_filter"
          ]
        },
        "lowercase": {
          "type": "custom",
          "tokenizer": "whitespace",
          "filter":   [
            "lowercase"
          ]
        },
        "phonenumber": {
          "type": "custom",
          "tokenizer": "standard",
          "filter":   [
            "lowercase"
          ],
          "char_filter": [
            "kill_spaces"
          ]
        }
      },
      "char_filter": {
        "kill_spaces": {
          "type": "mapping",
          "mappings": [
            "+47\\u0020 => \\u0020",
            "0\\u0020 => 0",
            "1\\u0020 => 1",
            "2\\u0020 => 2",
            "3\\u0020 => 3",
            "4\\u0020 => 4",
            "5\\u0020 => 5",
            "6\\u0020 => 6",
            "7\\u0020 => 7",
            "8\\u0020 => 8",
            "9\\u0020 => 9"
          ]
        }
      }
    }
  },
